            <subEntityTypes xmi:id="_nlngzGEDEd6k8rnx1Nuq4g" description="Means for efficient navigation in the documentation." name="navigation">
              <subEntityTypes xmi:id="_nlngzWEDEd6k8rnx1Nuq4g" description="Hyperlinked documents." name="crosslinking">
                <characterizedBy xmi:id="_nlngzmEDEd6k8rnx1Nuq4g" description="Sind die Querverweise in From von Hyperlinks realisiert?" impacts="_nlngz2EDEd6k8rnx1Nuq4g" property="_nlngYmEDEd6k8rnx1Nuq4g"/>
                <characterizedBy xmi:id="_nlng0GEDEd6k8rnx1Nuq4g" description="Alle Themen auf die sich ein Abschnitt oder ein Dokument bezieht und&#xA;die nicht in diesem selbst erklrt werden sollten durch Querverweise gekennzeichnet&#xA;sein.&#xA;&#xA;Hierfr sollten Richtlinien existieren, die festlegen auf welche Themen verwiesen&#xA;werden muss (z.B. bei der Beschreibung einer Klasse muss auf alle Klassen, von&#xA;denen geerbt wird, alle implementierten Schnittstellen, alle bekannten Klassen zu&#xA;denen eine Beziehung besteht oder die von dieser Klasse erben und alle Methoden&#xA;verwiesen werden.)" impacts="_nlng0WEDEd6k8rnx1Nuq4g" property="_nlngaGEDEd6k8rnx1Nuq4g"/>
                <characterizedBy xmi:id="_nlng0mEDEd6k8rnx1Nuq4g" description="Verfgt die Dokumentation ber Querverweise?" impacts="_nlng02EDEd6k8rnx1Nuq4g" property="_nlngbmEDEd6k8rnx1Nuq4g"/>
              </subEntityTypes>
              <subEntityTypes xmi:id="_nlng1GEDEd6k8rnx1Nuq4g" description="It should be possible to search the documentation for specific keywords." name="full_text_search">
                <characterizedBy xmi:id="_nlng1WEDEd6k8rnx1Nuq4g" description="Durchsucht die Volltextsuche-Funktion tatschlich die komplette Dokumentation?" impacts="_nlng1mEDEd6k8rnx1Nuq4g" property="_nlngaGEDEd6k8rnx1Nuq4g"/>
                <characterizedBy xmi:id="_nlng12EDEd6k8rnx1Nuq4g" description="Bietet die Dokumentation eine Volltext-Suche-Funktion?" impacts="_nlng2GEDEd6k8rnx1Nuq4g" property="_nlngbmEDEd6k8rnx1Nuq4g"/>
