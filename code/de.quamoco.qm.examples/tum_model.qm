              <subEntityTypes xmi:id="_nlnipGEDEd6k8rnx1Nuq4g" description="Dieser Fakt bescheibt die im Produkt verwendeten Operatoren." name="operator">
                <subEntityTypes xmi:id="_nlnipWEDEd6k8rnx1Nuq4g" description="Der Komma-Operator Bewirkt die sequenzielle Ausfhrung von zwei Ausdrcken." name="comma_operator">
