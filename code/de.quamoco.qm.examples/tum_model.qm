                  </subEntityTypes>
                  <characterizedBy xmi:id="_nlnih2EDEd6k8rnx1Nuq4g" description="Inconsisten usage of paramters" impacts="_nlniiGEDEd6k8rnx1Nuq4g _nlniiWEDEd6k8rnx1Nuq4g" property="_nlngbGEDEd6k8rnx1Nuq4g"/>
                  <characterizedBy xmi:id="_nlniimEDEd6k8rnx1Nuq4g" description="Hat diese Methode unerwartete Nebenwirkungen?" impacts="_nlnii2EDEd6k8rnx1Nuq4g _nlnijGEDEd6k8rnx1Nuq4g" property="_nlngcWEDEd6k8rnx1Nuq4g"/>
                  <characterizedBy xmi:id="_nlnijWEDEd6k8rnx1Nuq4g" description="Die Verwendung einer Parameterliste variabler Lnge ist nur in wenigen Fllen sinnvoll und muss eindeutig motiviert sein." impacts="_nlnijmEDEd6k8rnx1Nuq4g" property="_nlngdWEDEd6k8rnx1Nuq4g"/>
                </subEntityTypes>
                <subEntityTypes xmi:id="_nlnij2EDEd6k8rnx1Nuq4g" description="A method that is publicly accessable. In Java, a method declared ''public''." name="public_method">
                  <subEntityTypes xmi:id="_nlnikGEDEd6k8rnx1Nuq4g" description="The return value of the method." name="return_value">
                    <characterizedBy xmi:id="_nlnikWEDEd6k8rnx1Nuq4g" description="The method that is declared public does not return a reference to a private array, which could then be modified in unexpected ways." property="_nlngY2EDEd6k8rnx1Nuq4g"/>
